Algoritmo "semnome"

Var

   distancia: inteiro
   combustivel_g, consumo_m : real

Inicio
      escreval("Distancia percorrida em km: ")
      leia(distancia)
      escreval("Combustivel gasto: ")
      leia(combustivel_g)
      
      consumo_m <- distancia / combustivel_g
      
      escreval("consumo meido: ", consumo_m)
      
      


Fimalgoritmo